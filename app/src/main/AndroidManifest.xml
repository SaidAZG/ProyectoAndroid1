<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.ipn.escom.app_plantas_iswm">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_Plantas_ISWM">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".ConsultarPlantas"
            android:parentActivityName="mx.ipn.escom.app_plantas_iswm.Menu"></activity>
        <activity android:name=".RegistrarUsuario"
            android:parentActivityName="mx.ipn.escom.app_plantas_iswm.IniciarSesion"></activity>
        <activity android:name=".IniciarSesion"></activity>
        <activity android:name=".RegistrarPlanta"
            android:parentActivityName="mx.ipn.escom.app_plantas_iswm.Menu"></activity>
        <activity android:name=".Menu"></activity>
        <activity android:name=".More"
            android:parentActivityName="mx.ipn.escom.app_plantas_iswm.ConsultarPlantas"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>